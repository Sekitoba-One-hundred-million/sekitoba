    def stock_name_create( self ):
        today = datetime.date.today()
        year = str( int( today.year ) )
        month = str( int( today.month ) )
        day = str( int( today.day ) )

        if len( month ) == 1:
            month = "0" + month

        if len( day ) == 1:
            day = "0" + day

        stock_name = year + month + day + "_stock.pickle"
        return stock_name
